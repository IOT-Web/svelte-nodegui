    "glob": "^7.1.6",
    "rimraf": "^2.6.3",
    "rollup": "^2.26.10",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-typescript2": "^0.25.2",
    "svelte": "~3.24.1",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@nodegui/react-nodegui": "^0.9.0",
