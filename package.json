{
  "name": "svelte-desktop",
  "version": "0.1.0",
  "description": "Svelte integration for NodeGUI",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/shirakaba/svelte-nodegui.git"
  },
  "homepage": "https://github.com/shirakaba/svelte-nodegui",
  "keywords": [
